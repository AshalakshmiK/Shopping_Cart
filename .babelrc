{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": "3.6.4",
                "useBuiltIns":"entry",
                "targets":{
                    "browsers":[
                        ">1%",
                        "not ie 11",
                        "not op_mini all"
                    ]
                }
            }
        ],
       
        "@babel/preset-react"
    ],
    "plugins": [
        
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-syntax-dynamic-import"
        
    ]
}